pragma solidity ^0.8.0;

contract Evoting {

    function addNumbers(uint num1, uint num2) public pure returns (uint) {
        return num1 + num2;
    }

    function sunNumbers(uint num1, uint num2) public pure returns (uint) {
        return num1 - num2;
    }
}